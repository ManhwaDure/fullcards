{
  "name": "fullcards",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=15.0.0"
  },
  "scripts": {
    "build-backend": "tsoa spec-and-routes && openapi --name FullcardsApiClient --client axios --input tsoa-build/swagger.json --output src/apiClient && tsc --build tsconfig.backend.json",
    "build-frontend": "next build",
    "build": "yarn build-backend && yarn build-frontend",
    "generate-website": "next export",
    "start": "node ./backend-build/backend",
    "migrate-and-start": "yarn typeorm-migration-run && yarn start",
    "dev": "nodemon",
    "typeorm": "ts-node -P ./tsconfig.backend.json ./node_modules/typeorm/cli.js",
    "typeorm-schema-sync": "yarn typeorm schema:sync -d backend/database/dataSource.ts",
    "typeorm-schema-drop": "yarn typeorm schema:drop -d backend/database/dataSource.ts",
    "typeorm-migration-generate": "yarn typeorm migration:generate -d backend/database/dataSource.ts",
    "typeorm-migration-status": "TYPEORM_MIGRATION=yes yarn typeorm migration:show -d backend/database/dataSource.ts",
    "typeorm-migration-run": "TYPEORM_MIGRATION=yes yarn typeorm migration:run -d backend/database/dataSource.ts",
    "typeorm-migration-revert": "TYPEORM_MIGRATION=yes yarn typeorm migration:revert -d backend/database/dataSource.ts"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.27.2",
    "bulma": "^0.9.2",
    "classnames": "^2.2.6",
    "cuid": "^2.1.8",
    "express": "^4.17.3",
    "jose": "^4.8.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "next": "^12.1.5",
    "openid-client": "^5.1.5",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-quill": "^1.3.5",
    "react-visibility-sensor": "^5.1.1",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.32.5",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "swagger-ui-express": "^4.3.0",
    "tsoa": "^3.14.1",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@types/express": "^4.17.13",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/redis": "^2.8.28",
    "@types/swagger-ui-express": "^4.1.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "nodemon": "^2.0.16",
    "openapi-typescript-codegen": "^0.22.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
